<Window x:Class="Example14Project.SolidWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Example14Project"
        mc:Ignorable="d"
        Title="Solid Brush" Height="450" Width="800" Closing="Window_Closing">
    <Canvas>
        <Rectangle Height="136" Canvas.Left="102" Stroke="Black" Canvas.Top="91" Width="223" HorizontalAlignment="Left" VerticalAlignment="Top">
            <Rectangle.Fill>
                <SolidColorBrush Color="Aqua"/>
            </Rectangle.Fill>
        </Rectangle>
        <Ellipse Height="140" Canvas.Left="568" Stroke="Black" Canvas.Top="89" Width="154" HorizontalAlignment="Left" VerticalAlignment="Center">
            <Ellipse.Fill>
                <SolidColorBrush Color="Orange"/>
            </Ellipse.Fill>

        </Ellipse>

    </Canvas>
</Window>
